<?xml version='1.0' encoding='UTF-8'?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
   http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>pokazIndex</from-action>
            <from-outcome>pokazIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazLista</from-action>
            <from-outcome>pokazLista</from-outcome>
            <to-view-id>/lista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazDodaj</from-action>
            <from-outcome>pokazDodaj</from-outcome>
            <to-view-id>/dodaj.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/lista.xhtml</from-view-id>
        <navigation-case>
            <from-action>pokazIndex</from-action>
            <from-outcome>pokazIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazLista</from-action>
            <from-outcome>pokazLista</from-outcome>
            <to-view-id>/lista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazDodaj</from-action>
            <from-outcome>pokazDodaj</from-outcome>
            <to-view-id>/dodaj.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/dodaj.xhtml</from-view-id>
        <navigation-case>
            <from-action>pokazIndex</from-action>
            <from-outcome>pokazIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazLista</from-action>
            <from-outcome>pokazLista</from-outcome>
            <to-view-id>/lista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazDodaj</from-action>
            <from-outcome>pokazDodaj</from-outcome>
            <to-view-id>/dodaj.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/edit.xhtml</from-view-id>
        <navigation-case>
            <from-action>pokazIndex</from-action>
            <from-outcome>pokazIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazLista</from-action>
            <from-outcome>pokazLista</from-outcome>
            <to-view-id>/lista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>pokazDodaj</from-action>
            <from-outcome>pokazDodaj</from-outcome>
            <to-view-id>/dodaj.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <description>RowCounter</description>
        <managed-bean-name>rowCounter</managed-bean-name>
        <managed-bean-class>pl.karol10145.lab6.RowCounter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>Customer</description>
        <managed-bean-name>customer</managed-bean-name>
        <managed-bean-class>pl.pollub.Customer</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>DiscountCode</description>
        <managed-bean-name>discountCode</managed-bean-name>
        <managed-bean-class>pl.pollub.DiscountCode</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>MicroMarket</description>
        <managed-bean-name>microMarket</managed-bean-name>
        <managed-bean-class>pl.pollub.MicroMarket</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>CustomerHelper</description>
        <managed-bean-name>customerHelper</managed-bean-name>
        <managed-bean-class>pl.pollub.CustomerHelper</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>DiscountCodeHelper</description>
        <managed-bean-name>discountCodeHelper</managed-bean-name>
        <managed-bean-class>pl.pollub.DiscountCodeHelper</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>MicroMarketHelper</description>
        <managed-bean-name>microMarketHelper</managed-bean-name>
        <managed-bean-class>pl.pollub.MicroMarketHelper</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>